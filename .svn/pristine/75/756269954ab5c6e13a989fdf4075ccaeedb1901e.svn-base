<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>系统日志管理</title>
		<link rel="stylesheet" href="../../js/element/index.css">
		<script src="../../js/vue/vue.js"></script>
		<script src="../../js/vue/axios.min.js"></script>
		<script src="../../js/element/index.js"></script>
		<style>
			.text-title{
				line-height: 32px;
				float: right;
			}
			.right{
				float: right;
			}
			.el-form-item {
			    margin-bottom: 10px;
			}
			.el-dialog__body {
			    padding: 10px 20px;
			}
			.el-table td, .el-table th{
				text-align: center;
			}
		</style>
	</head>
	<body>
		<div id="app">
			<!-- 搜索栏 -->
			<el-row>
				<el-col :span="2">
					<span class="text-title">时间：</span>
				</el-col>
				<el-col :span="7">
					<el-date-picker
						size="small"
						value-format="yyyy-MM-dd HH:mm:ss"
					    v-model="json.times"
					    type="datetimerange"
					    range-separator="至"
					    start-placeholder="开始时间"
					    end-placeholder="结束时间">
					</el-date-picker> 
				</el-col>
				<el-col :span="2">
					<span class="text-title">类型：</span>
				</el-col>
				<el-col :span="3">
					<el-select size="small" v-model="json.log_type" placeholder="请选择">
						<el-option 
							v-for="item in type_options" 
							:key="item.value" 
							:label="item.label" 
							:value="item.value">
						</el-option>
					</el-select>
				</el-col>
				<el-col :span="2">
					<span class="text-title">操作结果：</span>
				</el-col>
				<el-col :span="3">
					<el-select size="small" v-model="json.result" placeholder="请选择">
						<el-option 
							v-for="item in result_options" 
							:key="item.value" 
							:label="item.label" 
							:value="item.value">
						</el-option>
					</el-select>
				</el-col>
				<el-col style="width: 15px;">&nbsp;</el-col>
				<el-col :span="3">
					<el-button type="primary" size="small" @click="search(0)">查询</el-button>
					<el-button type="danger" size="small" @click="checkDel">删除</el-button>
				</el-col>
			</el-row>
			<br>
			<!-- 表格栏 -->
			<el-row>
				 <el-table 
				    ref="multipleTable"
				    :data="datas"
					stripe
					border
					max-height="430px"
					size="small"
					@selection-change="handleSelectionChange"
					@row-click="handleRowClick"
				    style="width: 100%">
					<el-table-column
					    type="selection"
					    width="55">
					</el-table-column> 
					<el-table-column
						v-if="false"
					  	prop="log_id"
					  	label="id" >
					</el-table-column>
					<el-table-column
					   prop="log_type"
					   label="日志类型"
					   :formatter="formatLogType"
					   width="100">
					</el-table-column>
					<el-table-column
				        prop="title"
				        label="接口描述"
						 width="180">
				    </el-table-column>
				    <el-table-column
				        prop="log_time"
				        label="操作时间"
						 width="170">
				    </el-table-column>
					<el-table-column
					    prop="name"
					    label="操作者"
						 width="100">
					</el-table-column>
					<el-table-column
					  	prop="method"
					  	label="请求方法"
						width="300">
					</el-table-column>
					<el-table-column
						prop="result"
						label="操作结果"
						:formatter="formatResult"
						width="80">
					</el-table-column>
					<el-table-column
					  	prop="error"
					  	label="错误信息">
					</el-table-column>
				</el-table>
			</el-row>
			<!-- 分页 -->
			<el-row>
				<el-pagination
					@size-change="handleSizeChange"
				    @current-change="handleCurrentChange"
				    :current-page="1"
				    :page-sizes="[10,20,50,100]"
				    :page-size="10"
				    layout="total, sizes, prev, pager, next, jumper"
				    :total="total">
				</el-pagination>
			</el-row>
		</div>
	</body>
	<script src="../../js/config/config.js"></script>
	<script src="../../js/config/tool.js"></script>
	<script src="../js/systemLog-manage.js"></script>
</html>
